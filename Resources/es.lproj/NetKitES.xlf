    <body>
      <group resname="4D NetKit Messages">
        <trans-unit resname="ERR_4DNK_1">
          <source>Undefined or null parameters object.</source>
          <target>Parámetro Objeto null o indefinido.</target>
        </trans-unit>
        <trans-unit resname="ERR_4DNK_2">
          <source>"{attribute}" attribute is not defined or is empty.</source>
          <target>El atributo "{attribute}" no está definido o está vacío.</target>
        </trans-unit>
        <trans-unit resname="ERR_4DNK_3">
          <source>Unsupported value in "{attribute}" attribute.</source>
          <target>Valor no soportado en el atributo "{attribute}".</target>
        </trans-unit>
        <trans-unit resname="ERR_4DNK_4">
          <source>'GetToken' request returned an empty response.</source>
          <target>La petición 'GetToken' devolvió una respuesta vacía.</target>
        </trans-unit>
        <trans-unit resname="ERR_4DNK_5">
          <source>'GetToken' request returned an unexpected status code (received: {received} expected: {expected}).</source>
          <target>La petición 'GetToken' devolvió un código de estado inesperado (recibido: {received} esperado: {expected}).</target>
        </trans-unit>
        <trans-unit resname="ERR_4DNK_6">
          <source>'GetToken' request returned an empty 'authorizationCode'.</source>
          <target>La petición 'GetToken' devolvió un 'authorizationCode' vacío.</target>
        </trans-unit>
        <trans-unit resname="ERR_4DNK_7">
          <source>The port {port} is already used.</source>
          <target>El puerto {port} ya está en uso.</target>
        </trans-unit>
        <trans-unit resname="ERR_4DNK_8">
          <source>Request returned an unexpected response ({status} {explanation}: {message}).</source>
          <target>La petición devolvió una respuesta inesperada ({status} {explanation}: {message}).</target>
        </trans-unit>
        <trans-unit resname="ERR_4DNK_9">
          <source>Undefined or empty parameter {which} in function {function}.</source>
          <target>El parámetro {which} en la función {function} está vacío o sin definir.</target>
        </trans-unit>
        <trans-unit resname="ERR_4DNK_10">
          <source>Unsupported parameter type {which} in function {function}.</source>
          <target>No se soporta el tipo del parámetro {which} en la función {function}.</target>
        </trans-unit>
        <trans-unit resname="OAuth2_Response_Title">
          <target>4D NetKit Component</target>
          <source>4D NetKit Component</source>
        </trans-unit>
        <trans-unit resname="OAuth2_Response_Message">
          <source>Verification code received</source>
          <target>Código de verificación recibido</target>
        </trans-unit>
        <trans-unit resname="OAuth2_Error_Message">
          <source>Undefined error</source>
          <target>Error indefinido</target>
        </trans-unit>
        <trans-unit resname="OAuth2_Response_Details">
          <source>You may now close this window</source>
          <target>Ahora puede cerrar esta ventana</target>
        </trans-unit>
        <trans-unit resname="List_No_Next_Page">
          <source>No next page.</source>
          <target>No hay página siguiente.</target>
        </trans-unit>
        <trans-unit resname="List_No_Previous_Page">
          <source>No previous page.</source>
          <target>No hay página anterior.</target>
        </trans-unit>
      </group>
